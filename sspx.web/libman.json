{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.1.3",
      "destination": "wwwroot/",
      "files": [ "css/bootstrap.min.css", "js/bootstrap.min.js" ]
    },
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/js",
      "files": [ "jquery.slim.min.js" ]
    },
    {
      "library": "jquery-validate@1.18.0",
      "destination": "wwwroot/js",
      "files": [ "jquery.validate.min.js" ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/js/",
      "files": [ "jquery.validate.unobtrusive.min.js" ]
    },
    {
      "library": "popper.js@1.14.3",
      "destination": "wwwroot/js/popper",
      "files": [ "umd/popper.min.js" ]
    }
  ]
}